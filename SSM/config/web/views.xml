<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
        "-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
        "http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>
	<definition name="base.definition" template="/WEB-INF/jsp/templet.jsp" >
		<put-attribute name="title" value="" />
		<put-attribute name="header" value="/WEB-INF/jsp/header.jsp" />
		 <put-attribute name="menu" value="/WEB-INF/jsp/menu.jsp" /> 
		<put-attribute name="body" value="/WEB-INF/jsp/index.jsp" />
		<put-attribute name="footer" value="/WEB-INF/jsp/footer.jsp" />
	</definition>
	
	<definition name="user.login" template="/WEB-INF/jsp/user/login.jsp">
	</definition>
	
	<definition name="user.userlist" extends="base.definition">
	<put-attribute name="body" value="/WEB-INF/jsp/user/userlist.jsp" />
	</definition>
	
	<definition name="items.itemslist" extends="base.definition">
		<put-attribute name="body" value="/WEB-INF/jsp/items/itemslist.jsp" />
	</definition>
	<definition name="items.itemsadd" extends="base.definition">
		<put-attribute name="body" value="/WEB-INF/jsp/items/itemsadd.jsp" />
	</definition>
	<definition name="items.itemsupd" extends="base.definition">
		<put-attribute name="body" value="/WEB-INF/jsp/items/itemsupd.jsp" />
	</definition>
	
	
	<definition name="order.orderlist" extends="base.definition">
		<put-attribute name="body" value="/WEB-INF/jsp/order/orderlist.jsp" />
	</definition>
	<definition name="order.useritemslist" extends="base.definition">
		<put-attribute name="body" value="/WEB-INF/jsp/order/useritemslist.jsp" />
	</definition>
	<definition name="order.detaillist" extends="base.definition">
		<put-attribute name="body" value="/WEB-INF/jsp/order/detaillist.jsp" />
	</definition>
	
	
	
	<definition name="cart.cartlist" extends="base.definition">
		<put-attribute name="body" value="/WEB-INF/jsp/cart/cartlist.jsp" />
	</definition>
		
	
</tiles-definitions>